<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_picking_order_report_inherit_spt" inherit_id="stock.report_picking">
        <xpath expr="//div[@class='row mt48 mb32']/div[1]" position="after">
            <t t-if="o.branch_id">
                <div class="col-auto mw-100 mb-2">
                    <strong>Branch:</strong>
                    <p class="m-0" t-field="o.branch_id.display_name"/>
                </div>
            </t>
        </xpath>


    </template>

</odoo>
